{
  "name": "spryker-sdk/phpstan-spryker",
  "description": "Spryker PHPStan Extensions",
  "license": "MIT",
  "keywords": ["phpstan", "analyzing"],
  "homepage": "http://spryker.com",
  "authors": [
    {
      "name": "Spryker",
      "homepage": "http://spryker.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "phpstan/phpstan": "^0.10.2"
  },
  "require-dev": {
    "consistence/coding-standard": "^3.5",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "localheinz/composer-normalize": "~0.9.0",
    "phing/phing": "^2.16.0",
    "phpstan/phpstan-strict-rules": "^0.10",
    "slevomat/coding-standard": "^4.7.2",
    "spryker/code-sniffer": "*",
    "squizlabs/php_codesniffer": "^3.3.1"
  },
  "autoload": {
    "psr-4": {
      "PHPStan\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/phpstan/vendor/ ./",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/phpstan/vendor/ ./"
  },
  "support": {
    "source": "https://github.com/spryker-sdk/phpstan-spryker"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.0-dev"
    }
  },
  "config": {
    "use-include-path": true,
    "process-timeout": 600,
    "sort-packages": true
  }
}
